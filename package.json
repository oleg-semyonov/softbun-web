{
  "name": "softbun-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-styles": "npx stylelint **.css",
    "lint-styles:fix": "npx stylelint **.css --fix",
    "prettier": "prettier --check './**/*.(js|ts|tsx|css)'",
    "prettier:fix": "npm run prettier -- --write --no-list-different",
    "typecheck": "tsc --noEmit --incremental",
    "check": "npm run lint && npm run typecheck && npm run lint-styles",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "contentful": "^9.1.10",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@prezly/eslint-config": "^4.1.0",
    "@svgr/webpack": "^6.2.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-tailwindcss": "^0.0.3",
    "tailwindcss": "^3.0.15",
    "typescript": "4.5.5"
  }
}
