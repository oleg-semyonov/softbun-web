  "devDependencies": {
    "@svgr/webpack": "^6.2.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.0.8",
