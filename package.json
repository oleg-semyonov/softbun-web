    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/typography": "^0.5.8",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "stylelint": "^14.15.0",
    "stylelint-config-tailwindcss": "^0.0.6",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  }
}
