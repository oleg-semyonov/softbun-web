{
  "name": "softbun-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-styles": "npx stylelint **.css",
    "lint-styles:fix": "npx stylelint **.css --fix",
    "prettier": "prettier --check './**/*.(js|ts|tsx|css)'",
    "prettier:fix": "npm run prettier -- --write --no-list-different",
    "typecheck": "tsc --noEmit --incremental",
    "check": "npm run lint && npm run typecheck && npm run lint-styles",
    "stylelint-check": "stylelint-config-prettier-check",
    "generate": "graphql-codegen --config .graphql.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@contentful/rich-text-types": "^15.11.1",
    "classnames": "^2.3.1",
    "graphql": "^16.3.0",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-markdown": "^8.0.0",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/fragment-matcher": "3.2.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@prezly/eslint-config": "^4.1.0",
    "@svgr/webpack": "^6.2.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-tailwindcss": "^0.0.3",
    "tailwindcss": "^3.0.15",
    "typescript": "4.5.5"
  }
}
