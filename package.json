    "eslint-plugin-sort-export-all": "^1.2.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-tailwindcss": "^0.0.3",
    "tailwindcss": "^3.0.15",
    "typescript": "4.5.5"
