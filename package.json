{
  "name": "softbun-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-styles": "npx stylelint **.css",
    "lint-styles:fix": "npx stylelint **.css --fix",
    "prettier": "prettier --check './**/*.(js|ts|tsx|css)'",
    "prettier:fix": "npm run prettier -- --write --no-list-different",
    "typecheck": "tsc --noEmit --incremental",
    "check": "npm run lint && npm run typecheck && npm run lint-styles",
    "stylelint-check": "stylelint-config-prettier-check",
    "generate": "graphql-codegen --config .graphql.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@contentful/rich-text-types": "^15.13.2",
    "classnames": "^2.3.2",
    "graphql": "^16.6.0",
    "lodash-es": "^4.17.21",
    "next": "^13.0.1-canary.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-markdown": "^8.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/typescript": "^2.7.4",
    "@prezly/eslint-config": "^4.2.1-2",
    "@svgr/webpack": "^6.5.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.21",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-tailwindcss": "^0.0.6",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4"
  }
